import java.util.Stack;

public class Deck {
	private Stack<Card>deck;
	
	public Deck(){
		deck = new Stack<Card>();
		deck.push(new Card(Suit.CLUB, "A"));
		deck.push(new Card(Suit.DIAMOND, "A"));
		deck.push(new Card(Suit.HEART, "A"));
		deck.push(new Card(Suit.SPADE, "A"));
		deck.push(new Card(Suit.CLUB, "A"));
		deck.push(new Card(Suit.CLUB, "A"));
		
		
	}
}
