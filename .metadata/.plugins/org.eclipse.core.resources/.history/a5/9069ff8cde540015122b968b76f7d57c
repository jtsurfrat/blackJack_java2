import java.util.Stack;

public class Hand {
	private Stack<Card> hand;
	private int score = 0;
	
	
	public Stack<Card> getHand() {
		return hand;
	}


	public void setHand(Stack<Card> hand) {
		this.hand = hand;
	}


	public int getScore() {
		return score;
	}


	public void setScore(int score) {
		this.score = score;
	}


	public hand(){
		hand = new Stack<Card>();
	}
	
	public int calculateScore(){
		return 0;
	}

}
